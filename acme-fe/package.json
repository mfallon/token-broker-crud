{
  "name": "item-crud",
  "version": "0.0.2",
  "description": "A generic item crud frontend built in react",
  "repository": "",
  "private": true,
  "scripts": {
    "build": "parcel build -d ./dist index.html",
    "start": "parcel index.html",
    "dev": "cd ../broker-api && npm run start"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "date-fns": "^2.16.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.12.4",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
